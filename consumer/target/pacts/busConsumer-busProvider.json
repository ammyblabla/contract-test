{
  "provider": {
    "name": "busProvider"
  },
  "consumer": {
    "name": "busConsumer"
  },
  "interactions": [
    {
      "description": "get bus",
      "request": {
        "method": "GET",
        "path": "/bus/HammerSmith/613"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": {
          "eta": 614,
          "nr": 613,
          "station": "HammerSmith"
        },
        "matchingRules": {
          "body": {
            "$.eta": {
              "matchers": [
                {
                  "match": "number"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "test state"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.10"
    }
  }
}